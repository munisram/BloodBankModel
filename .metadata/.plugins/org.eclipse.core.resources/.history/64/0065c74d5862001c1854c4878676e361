package com.bloodbank.model;

import java.util.Objects;

public class BloodPriceModel {
	
	private String bloodType;
	private 
	public BloodPriceModel() {
		super();
		// TODO Auto-generated constructor stub
	}
	public BloodPriceModel(String bloodType, int price) {
		super();
		this.bloodType = bloodType;
		this.price = price;
	}
	@Override
	public String toString() {
		return "BloodPriceModel [bloodType=" + bloodType + ", price=" + price + "]";
	}
	@Override
	public int hashCode() {
		return Objects.hash(bloodType, price);
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		BloodPriceModel other = (BloodPriceModel) obj;
		return Objects.equals(bloodType, other.bloodType) && price == other.price;
	}
	public String getBloodType() {
		return bloodType;
	}
	public void setBloodType(String bloodType) {
		this.bloodType = bloodType;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	private int price;

}
