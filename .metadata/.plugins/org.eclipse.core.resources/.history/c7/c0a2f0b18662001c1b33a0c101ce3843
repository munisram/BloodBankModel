package com.bloodbank.model;

import java.util.Objects;

public class BillingModel {
	
	private String bloodType;
	public BillingModel(String bloodType, BloodStack quantity, BloodPriceModel price) {
		super();
		this.bloodType = bloodType;
		this.quantity = quantity;
		this.price = price;
	}
	@Override
	public String toString() {
		return "BillingModel [bloodType=" + bloodType + ", quantity=" + quantity + ", price=" + price + "]";
	}
	@Override
	public int hashCode() {
		return Objects.hash(bloodType, price, quantity);
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		BillingModel other = (BillingModel) obj;
		return Objects.equals(bloodType, other.bloodType) && Objects.equals(price, other.price)
				&& Objects.equals(quantity, other.quantity);
	}
	public String getBloodType() {
		return bloodType;
	}
	public void setBloodType(String bloodType) {
		this.bloodType = bloodType;
	}
	public BloodStack getQuantity() {
		return quantity;
	}
	public void setQuantity(BloodStack quantity) {
		this.quantity = quantity;
	}
	public BloodPriceModel getPrice() {
		return price;
	}
	public void setPrice(BloodPriceModel price) {
		this.price = price;
	}

	
	
	
}
