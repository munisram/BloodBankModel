package com.bloodbank.model;

import java.util.Objects;

public class BloodDetailsModel {
	private Long adharcard;
	private int unit;
	private String bloodType;
	public Long getAdharcard() {
		return adharcard;
	}
	public void setAdharcard(Long adharcard) {
		this.adharcard = adharcard;
	}
	public int getQuantity() {
		return unit;
	}
	public void setQuantity(int quantity) {
		this.unit = quantity;
	}
	public String getBloodType() {
		return bloodType;
	}
	public void setBloodType(String bloodType) {
		this.bloodType = bloodType;
	}
	@Override
	public int hashCode() {
		return Objects.hash(adharcard, bloodType, unit);
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		BloodDetailsModel other = (BloodDetailsModel) obj;
		return Objects.equals(adharcard, other.adharcard) && Objects.equals(bloodType, other.bloodType)
				&& unit == other.unit;
	}
	@Override
	public String toString() {
		return "BloodDetailsModel [adharcard=" + adharcard + ", quantity=" + unit + ", bloodType=" + bloodType
				+ "]";
	}
	public BloodDetailsModel(Long adharcard, int quantity, String bloodType) {
		super();
		this.adharcard = adharcard;
		this.unit = quantity;
		this.bloodType = bloodType;
	}
	public BloodDetailsModel() {
		super();
		// TODO Auto-generated constructor stub
	}
	

}
