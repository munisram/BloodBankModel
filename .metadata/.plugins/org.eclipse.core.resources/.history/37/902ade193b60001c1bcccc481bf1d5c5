package com.bloodbank.Dao;

import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.SQLException;

import com.bloodbank.model.BloodDetailsModel;

public class BloodDetailsDao {
	
	
	
	public int insertBloodDetails(BloodDetailsModel details) {
		int n=0;
		ConnectionUtil connection=new ConnectionUtil();
		try {
			Connection con=connection.getConnection();
			String query="insert into blood_details values(?,?,?,?)";
			PreparedStatement pstmt=con.prepareStatement(query);
			pstmt.setString(1, details.getBloodType());
			pstmt.setLong(2, details.getAdharcard());
			pstmt.setInt(1, details.getQuantity());
			pstmt.executeUpdate();
			
			
			
		} catch (ClassNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		
		
		
		
		
		
		
		return 0;
		
	}

}
